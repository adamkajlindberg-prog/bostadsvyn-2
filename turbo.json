{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env*"
  ],
  "globalEnv": [
    "AI_CHAT_AGENT",
    "AUTH_FROM_EMAIL",
    "AUTH_SECRET",
    "DATABASE_URL",
    "DISABLE_INDEXING",
    "EMBEDDING_DIMENSIONALITY",
    "GEMINI_API_KEY",
    "GOOGLE_OAUTH_CLIENT_SECRET",
    "IDURA_CLIENT_ID",
    "IDURA_CLIENT_SECRET",
    "IDURA_CLIENT_URL",
    "NEXT_PUBLIC_BUCKET_URL",
    "NEXT_PUBLIC_CUSTOMER_SERVICE_EMAIL",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "NEXT_PUBLIC_GOOGLE_OAUTH_CLIENT_ID",
    "NEXT_PUBLIC_WEB_URL",
    "OPENAI_API_KEY",
    "R2_ACCESS_KEY_ID",
    "R2_ACCESS_SECRET",
    "R2_BUCKET",
    "R2_ENDPOINT",
    "R2_REGION",
    "REDIS_PREFIX",
    "REDIS_TOKEN",
    "REDIS_URL",
    "RESEND_API_KEY",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "TRAFIKVERKET_API_KEY"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ],
  "tasks": {
    "//#format": {
      "cache": false
    },
    "//#lint": {},
    "//#lint-fix": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "cache": false,
      "outputs": [
        "tsconfig.tsbuildinfo",
        ".tsbuildinfo"
      ]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}